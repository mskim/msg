<p id="notice"><%= notice %></p>

<h1>Listing Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Menu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><img  src="<%= picture.picture_path %>"></img></td>

    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Picture', new_picture_path %>
